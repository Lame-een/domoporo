    color: #ffffff;
    background-color: #333334;
    border: none;
}

QComboBox::hover{
	background-color: #464649
}

QComboBox QScrollBar:vertical {
    background-color: #2D2D30;
    margin: 0px;
}

QComboBox QScrollBar::handle:vertical {
    background-color: #333334;
    border-radius: 2px;
    border-color: #ffffff;
    min-height: 20px;
}

QComboBox QScrollBar::handle:hover {
    background-color: #3A3A3A;
}

